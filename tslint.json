{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-react-a11y"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": [
      true,
      "always",
      "strict-bound-class-methods"
    ],
    "trailing-comma": false,
    "quotemark": [
      true,
      "single",
      "jsx-double",
      "avoid-template",
      "avoid-escape"
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "jsx-boolean-value": false,
    "jsx-no-multiline-js": false,
    "jsx-no-lambda": false
  },
  "rulesDirectory": []
}